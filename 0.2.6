#include <iostream>
#include <cmath>

using namespace std;
int main()
{
    double a, b, c, r, s;
    cout << "a:"; cin >> a;
    cout << "b:"; cin >> b;
    cout << "c:"; cin >> c;
    cout << "r:"; cin >> r;
    s = 2 * pow(r, 2) * sin(a) * sin(b) * sin(c);
    cout << endl << "s=" << s << endl;
}
